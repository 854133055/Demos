<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/root"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/rotate_icon_launche"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="anlaiye.com.cn.performancedemo.MainActivity">

    <include
        layout="@layout/incldue_title"
        android:layout_width="match_parent"
        android:layout_height="60dp"/>

    <include layout="@layout/merge_title"/>

    <TextView
        android:id="@+id/testAllocationTracker"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="s"/>

    <TextView
        android:id="@+id/testAllocationTrackerSb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="sb"/>


    <TextView
        android:id="@+id/testLL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LinearLayout性能"/>

    <TextView
        android:id="@+id/testRL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Relative性能"/>

    <TextView
        android:id="@+id/testCons"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ConstraintLayout性能"/>

</LinearLayout>
